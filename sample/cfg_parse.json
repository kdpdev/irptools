{
  "target" : {
    "folder": {
      "path": "./parsed",
      "cleanupIfExists": true,
      "withoutCreationTime": true
    },
    "withStat": true,
    "prettyJsonPrint": false,
    "keepSourceField": false
  },

  "sources": {
    "visio": {
      "skip": true,
      "type": "visio",
      "path": "./data/visio"
    },

    "fz": {
      "skip": false,
      "type": "fz",
      "path": "./data/fz",
      "options": {
        "ignoreAllUnsupportedProtocolError": true
      }
    }
  }
}
